{
    "name": "amirsasani/reporting-system",
    "description": "Laravel Reporting System package",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Amirsasani\\ReportingSystem\\": "src/",
            "Amirsasani\\ReportingSystem\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Amirsasani\\ReportingSystem\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Amir Sasani",
            "email": "sasani.amir.1998@gmail.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^6.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-f": "vendor/bin/phpunit --filter"
    }
}
